    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./images/Lacochinita.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.alexpizarro3.myfirstapp"
    },
    "web": {
      "favicon": "./images/Lacochinita.png"
    }
